{
  "name": "landing",
  "version": "1.0.0",
  "description": "landing build",
  "scripts": {
    "dev": "rm -rf dist && npm run development",
    "development": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/webpack-mix/setup/webpack.config.js",
    "watch": "NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/webpack-mix/setup/webpack.config.js",
    "hot": "NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/webpack-mix/setup/webpack.config.js",
    "prod": "rm -rf dist && npm run production",
    "production": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/webpack-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bahir24/landing.git"
  },
  "keywords": [
    "landing",
    "site"
  ],
  "author": "MihailBahirev",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bahir24/landing/issues"
  },
  "homepage": "https://github.com/bahir24/landing#readme",
  "devDependencies": {
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.1.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12",
    "webpack-mix": "^3.0.0"
  },
  "dependencies": {
    "pug": "^3.0.2"
  }
}
